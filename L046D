#include <stdio.h>

int main() {
    int A[15];
    int B[15];
    int i;

    printf("Digite 15 elementos para a matriz A:\n");
    for (i = 0; i < 15; i++) {
        printf("A[%d]: ", i + 1);
        scanf("%d", &A[i]);
    }

    for (i = 0; i < 15; i++) {
        B[i] = A[i] * A[i];
    }

    printf("\nOs elementos da matriz A são:\n");
    for (i = 0; i < 15; i++) {
        printf("A[%d]: %d\n", i + 1, A[i]);
    }

    printf("\nOs elementos da matriz B (quadrado de A) são:\n");
    for (i = 0; i < 15; i++) {
        printf("B[%d]: %d\n", i + 1, B[i]);
    }

    return 0;
}
