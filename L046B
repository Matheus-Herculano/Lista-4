#include <stdio.h>

int main() {
    int A[8];
    int B[8];
    int i;

    printf("Digite 8 elementos para a matriz A:\n");
    for (i = 0; i < 8; i++) {
        printf("Elemento %d: ", i + 1);
        scanf("%d", &A[i]);
    }

    for (i = 0; i < 8; i++) {
        B[i] = A[i] * 3;
    }

    printf("\nOs elementos da matriz B (A multiplicado por 3) sÃ£o:\n");
    for (i = 0; i < 8; i++) {
        printf("Elemento %d: %d\n", i + 1, B[i]);
    }

    return 0;
}
